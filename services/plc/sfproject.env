# PLC Configuration for sfproject.net domains
# Service configuration
PLC_PUBLIC_URL="https://plc.sfproject.net"
PLC_PORT="2582"

# Database configuration
PLC_DB_POSTGRES_URL="postgresql://username:password@localhost:5432/plc"
PLC_DB_POSTGRES_SCHEMA="plc"

# Admin configuration
PLC_ADMIN_PASSWORD="your-admin-password"

# Signing key (generate a secure 64-char hex string)
PLC_SIGNING_KEY_HEX="your-64-char-hex-signing-key"

# Environment
NODE_ENV="production"
PLC_VERSION="1.0.0"

# Optional: Rate limiting
PLC_RATE_LIMIT_ENABLED=true
PLC_RATE_LIMIT_REQUESTS_PER_MINUTE=100
